{
  "name": "mathrix/lumen-jwt-auth",
  "description": "A small library to handle authentication using JWT.",
  "version": "1.2.0-rc1",
  "type": "library",
  "keywords": [
    "mathrix",
    "php",
    "lumen",
    "jwt",
    "authentication"
  ],
  "homepage": "https://github.com/mathrix-education/lumen-jwt-auth",
  "license": "proprietary",
  "authors": [
    {
      "name": "Mathieu Bour",
      "email": "mathieu@mathrix.fr",
      "homepage": "https://www.linkedin.com/in/mathieu-bour",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mathieu@mathrix.fr",
    "issues": "https://github.com/mathrix-education/lumen-jwt-auth/issues",
    "wiki": "https://github.com/mathrix-education/lumen-jwt-auth/wiki",
    "source": "https://github.com/mathrix-education/lumen-jwt-auth",
    "docs": "https://github.com/mathrix-education/lumen-jwt-auth/wiki"
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "mathrix/lumen-zero": "dev-master",
    "ramsey/uuid": "^3.8",
    "web-token/jwt-core": "^1.3",
    "web-token/jwt-key-mgmt": "^1.3",
    "web-token/jwt-signature-algorithm-ecdsa": "^1.3",
    "web-token/jwt-signature-algorithm-rsa": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Mathrix\\Lumen\\JWT\\Auth\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:mathrix-education/lumen-zero.git"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
